{
  "name": "@uk-export-finance/typescript-package",
  "version": "1.0.3",
  "description": "TypeScript Package ready to be published on NPM and Github packages",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "build": "tsc",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "eslint --ext .ts src/ --fix",
    "prepare": "husky install"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "Gabriel Ignat",
  "license": "MIT",
  "homepage": "https://github.com/UK-Export-Finance/typescript-package",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UK-Export-Finance/typescript-package.git"
  },
  "bugs": {
    "url": "https://github.com/UK-Export-Finance/typescript-package/issues"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4"
  }
}
